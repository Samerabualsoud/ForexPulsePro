modules = ["python-3.11", "postgresql-16", "python3"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "glibcLocales", "libiconv", "libxcrypt", "openssl", "pkg-config", "rustc"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Forex Signal Dashboard"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FastAPI Backend"

[[workflows.workflow]]
name = "Forex Signal Dashboard"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "BACKEND_URL=http://localhost:8080 streamlit run app.py --server.port 5000 --server.address 0.0.0.0"
waitForPort = 5000

[[workflows.workflow]]
name = "FastAPI Backend"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "BACKEND_PORT=8080 BACKEND_HOST=0.0.0.0 python -m uvicorn backend.main:app --host 0.0.0.0 --port 8080"
waitForPort = 8080

